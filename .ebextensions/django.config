option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: keuka/wsgi.py

packages:
  yum:
    libjpeg-turbo-devel: []
    libpng-devel: []
    git: []
    postgresql93-devel: []

container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python keuka/manage.py migrate --noinput"
    leader_only: true